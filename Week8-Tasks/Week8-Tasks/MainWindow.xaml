        <ListBox x:Name="SuppliersBox" HorizontalAlignment="Left" Height="131" Margin="214,64,0,0" VerticalAlignment="Top" Width="360" SelectionChanged="SuppliersBox_SelectionChanged"/>
        <ListBox x:Name="CountryBox" HorizontalAlignment="Left" Height="131" Margin="579,64,0,0" VerticalAlignment="Top" Width="200" SelectionChanged="CountryBox_SelectionChanged"/>
        <ListBox x:Name="ProductsBox" HorizontalAlignment="Left" Height="146" Margin="10,263,0,0" VerticalAlignment="Top" Width="769"/>
        <Label Content="Stock Level" HorizontalAlignment="Left" Margin="66,33,0,0" VerticalAlignment="Top"/>
        <Label Content="Suppliers" HorizontalAlignment="Left" Margin="359,33,0,0" VerticalAlignment="Top"/>
        <Label Content="Country" HorizontalAlignment="Left" Margin="652,33,0,0" VerticalAlignment="Top"/>
        <Label Content="Products" HorizontalAlignment="Left" Margin="359,232,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
